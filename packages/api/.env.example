# API Environment Variables
# Copy this file to .env and fill in your values

# Server Configuration
PORT=9006
NODE_ENV=development

# CoinGecko API
COINGECKO_API_KEY=

# Monitoring Configuration
MONITORING_INTERVAL=5
SYMBOLS_LIMIT=20
ENABLE_ALERTS=true

# Logging
LOG_LEVEL=info

# Cache Configuration
CACHE_TTL=120000

# Rate Limiting
RATE_LIMIT=30
RATE_WINDOW=60000
